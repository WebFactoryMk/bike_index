<% if @public_image.new_record? %>
  window.BikeIndexAlerts.add("error", "<%=t :public-images.failed-to-upload %>: #{j @public_image.errors.full_messages.join(', ').html_safe}");
<% else %>
 $("#public_images").append("<%= j(render partial: '/public_images/revised_public_image', locals: { image: @public_image }) %>");
<% end %>
